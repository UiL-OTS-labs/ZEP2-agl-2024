// Symbolic definition of item type.
enum ItemType
{
    NON_WORD,                   // non-word
    REAL_WORD,                  // real word
}


// Item record used for the test phase item table.
record TestItem
{
    int         id;             // item id
    ItemType    type;           // item type
    string      word;           // stimulus word
}


